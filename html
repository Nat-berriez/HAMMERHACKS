        <img src="https://www.cityrentatruck.com/wp-content/uploads/2023/02/Transparent.png">

        <blockquote>"There's no place like home." - wizard of Oz</blockquote>
        <button onclick="changeQuote()">Change Quote/button>

        <script>
                var index = 0;

                function changeQuote() {
                    var quotes = [
                    `"I really like web development!" - Dave Smith-Hayes`,
                    `"I am enjoying this a lot" - Me`
                    ];
                    
                    var blackquote = document.querySelector("blockquote");
                    blockquote.innerHTML = quotes[index];

                    index++;
                    if (index >= 4) {
                        index = 0;
                    }
                }
         </script>
